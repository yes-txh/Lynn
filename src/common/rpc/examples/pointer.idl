module Demo
{

/// 测试出入参数为指针
interface Pointer
{
    /// 参数不做任何改动返回，入参数类型为指针
    [timeout(100)]
    std::string Echo1(std::string msg);

    /// 转为大写，出入参数为指针
    [timeout(100)]
    void ToUpper([in, out] std::string* msg);

    /// 返回第二个参数，类型为指针
    [timeout(100), amd]
    void Echo2(const std::string& msg, [out] std::string* result);

    [timeout(100)]
    void Echo3(const std::string& msg, [out] std::string* result);
}

}
